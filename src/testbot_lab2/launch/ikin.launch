<launch>
    <param name="robot_description" command="xacro --inorder '$(find testbot_lab2)/urdf/second.urdf'"/>
    <arg name="rvizconfig" default="$(find testbot_lab2)/rviz/defaultikin.rviz" />
    <param name="use_gui" value="true"/>

    <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
    <node name="ikin" pkg="testbot_lab2" type="ikin.py" output="screen"/>
    <node name="oint" pkg="testbot_lab2" type="oint_script.py" output="screen"/>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />
</launch>
